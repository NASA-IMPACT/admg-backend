location /static/ {
  alias /home/app/web/staticfiles/;
  add_header Access-Control-Allow-Origin *;
}

location /media/ {
  alias /home/app/web/media/;
  add_header Access-Control-Allow-Origin *;
}

location ~ \.css {
  add_header  Content-Type    text/css;
}

location ~ \.js {
  add_header  Content-Type    application/x-javascript;
}
